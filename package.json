  "type": "module",
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.0",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "purecss": "^3.0.0",
    "sass": "^1.89.2",
    "sass-embedded": "^1.89.2",
    "sass-loader": "^16.0.5",
    "serve": "^14.2.4",
    "style-loader": "^4.0.0",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage --run",
    "publish": "npm-run-all -s javascript:production watch:server",
    "watch": "npm-run-all -p watch:*",
    "watch:server": "serve ./docs/",
    "javascript:dev": "webpack --config webpack.config.script.js --mode development",
    "watch:javascript": "npm run javascript:dev -- --watch",
    "javascript:production": "webpack --config webpack.config.script.js --mode production --progress --color",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,css,md}": "prettier --write"
  }
