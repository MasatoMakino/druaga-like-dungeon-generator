  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@vitest/coverage-v8": "^1.1.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "purecss": "^3.0.0",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.3",
    "serve": "^14.2.1",
    "style-loader": "^3.3.3",
    "vitest": "^1.2.2",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage --run",
    "publish": "npm-run-all -s javascript:production watch:server",
    "watch": "npm-run-all -p watch:*",
    "watch:server": "serve ./docs/",
    "javascript:dev": "webpack --config webpack.config.script.js --mode development",
    "watch:javascript": "npm run javascript:dev -- --watch",
    "javascript:production": "webpack --config webpack.config.script.js --mode production --progress --color",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,css,md}": "prettier --write"
  }
}
